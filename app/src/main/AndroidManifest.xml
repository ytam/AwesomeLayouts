<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ytam.awesomelayouts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ytam.awesomelayouts.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ytam.awesomelayouts.ItemLayout" />
        <activity android:name="com.example.ytam.awesomelayouts.Item2" />
        <activity android:name="com.example.ytam.awesomelayouts.ExpanableLayout" />
        <activity android:name="com.example.ytam.awesomelayouts.HomeScreen" />
        <activity
            android:name="com.example.ytam.awesomelayouts.Login"
            android:theme="@style/NoActionBar" />
        <activity android:name="com.example.ytam.awesomelayouts.Home2Activity"></activity>
    </application>

</manifest>